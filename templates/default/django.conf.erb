<IfModule mod_wsgi.c>
  WSGIDaemonProcess django python-home=<%= @path_to_python_env %> python-path=<%= @path_to_manage_dir %>
  WSGIProcessGroup django
  WSGIScriptAlias / <%= @path_to_wsgi_py %>
  # WSGISocketPrefix run/wsgi
  WSGISocketPrefix <%= @wsgi_socket_prefix %>
</IfModule>
