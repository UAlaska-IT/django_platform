# This file is managed with Chef. For changes to persist, edit django_platform/templates/django.conf.erb

Alias /static <%= @path_to_static_directory %>
<Directory <%= @rel_path_to_static_directory %>>
  Require all granted
</Directory>

<Directory <%= @rel_path_to_site_directory %>>
  <Files wsgi.py>
    Require all granted
  </Files>
</Directory>

WSGIDaemonProcess django python-path=<%= @path_to_http_root %> python-home=<%= @path_to_venv %>>
WSGIProcessGroup django
WSGIScriptAlias / <%= @path_to_wsgi_py %>
WSGISocketPrefix run/wsgi
